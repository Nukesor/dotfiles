# A script that backups all my files in a specific Folder for Seafile


rsync --update --delete --recursive --progress ~/.Skype ~/Seafile/Backup/
rsync --update --delete --recursive --progress ~/.ssh ~/Seafile/Backup/
rsync --update --delete --recursive --progress ~/.thunderbird ~/Seafile/Backup/
rsync --update --delete --recursive --progress ~/Dokument ~/Seafile/Backup/
rsync --update --delete --recursive --progress ~/torrent ~/Seafile/Backup/
rsync --update --delete --recursive --progress ~/.local/lost-aurora-1080p.jpg ~/Seafile/Backup/
rsync --update --delete --recursive --progress ~/.LUFTRAUSERS ~/Seafile/Backup/

